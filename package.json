{
  "name": "express-backend",
  "version": "1.0.0",
  "description": "ecommerce project",
  "scripts": {
    "start": "npm run transpile && node server.js",
    "dev": "concurrently \"npm:watch\" \"npm:serve\"",
    "serve": "nodemon server.js",
    "watch": "tsc -w",
    "transpile": "tsc",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts",
    "format": "prettier --write src/**/*.ts --config ./.prettierrc",
    "prepare": "husky install"
  },
  "keywords": [
    "Node",
    "Typescript",
    "MongoDB",
    "Javascript"
  ],
  "author": "Krutarth Patel",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.1.0",
    "concurrently": "^8.2.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-rate-limiter": "^1.3.1",
    "express-session": "^1.18.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "mongodb": "^6.5.0",
    "mongoose": "^8.3.2",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.1.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/concurrently": "^7.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.7",
    "@types/passport-google-oauth20": "^2.0.14",
    "@types/passport-jwt": "^4.0.1",
    "@types/uuid": "^9.0.8",
    "@types/winston": "^2.4.4",
    "colors": "^1.4.0",
    "dotenv": "^16.4.5",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "morgan": "^1.10.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "winston": "^3.13.0"
  }
}
